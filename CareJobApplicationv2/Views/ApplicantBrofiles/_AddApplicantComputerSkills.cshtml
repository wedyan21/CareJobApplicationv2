@model CareJobApplicationv2.Models.ApplicantComputerSkills

@{
    ViewData["Title"] = "Create";
    // Layout = "~/Views/Shared/_AdminLayout.cshtml";
    Layout = null;
}


<div class="row">
    <div class="col-md-12">
        <form asp-action="AddApplicantComputerSkill" method="post" onsubmit="return SubmitForm(this,'ApplicantComputerSkills')">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row mt-5 mb-5">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label asp-for="SkillId" class="control-label"></label>
                        <select asp-for="SkillId" class="form-control" asp-items="ViewBag.SkillId"></select>
                    </div>
                </div>

                <div class="col-lg-6 mb-3">
                    <div class="form-group">
                        <label asp-for="SkillLevelId" class="control-label"></label>
                        <select asp-for="SkillLevelId" class="form-control" asp-items="ViewBag.SkillLevelId"></select>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
